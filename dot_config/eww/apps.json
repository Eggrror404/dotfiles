{
  "workspaces": [
    {
      "class": "firefox",
      "exec": "firefox",
      "fallback": [
        {
          "class": "firefox-nightly"
        }
      ]
    },
    {
      "class": "org.gnome.Nautilus",
      "exec": "nautilus --new-window"
    },
    {
      "class": "fakeapp.window",
      "exec": "alacritty --working-directory ~",
      "fallback": [
        {
          "class": "Alacritty"
        }
      ]
    },
    {
      "class": "Spotify",
      "exec": "LD_PRELOAD=/usr/lib/spotifywm.so spotify",
      "fallback": [
        {
          "class": "YouTube Music"
        },
        {
          "class": "firefox",
          "title": "Now Playing | Gensokyo Radio — Mozilla Firefox"
        }
      ]
    },
    {
      "class": "fakeapp.game",
      "exec": "steam",
      "fallback": [
        {
          "class": "Steam"
        },
        {
          "class": "org.prismlauncher.PrismLauncher"
        },
        {
          "class": "Minecraft\\*.*"
        },
        {
          "class": "osu!"
        }
      ]
    },
    {
      "class": "WebCord",
      "exec": "webcord --enable-features=UseOzonePlatform --ozone-platform=wayland",
      "fallback": [
        {
          "class": "revolt-desktop"
        }
      ]
    }
  ],
  "_friendly_reminder": "In the `apps` array, latter elements take priority.",
  "apps": [
